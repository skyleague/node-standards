    "prepare": "husky || true",
    "test": "vitest run"
  },
  "dependencies": {},
  "devDependencies": {
    "@skyleague/node-standards": ">=8.2.4",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=22"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org"
  },
  "node-standards": {
    "extends": ["library", "docusaurus"]
  }
