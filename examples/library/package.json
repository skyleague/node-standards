{
  "name": "@skyleague/__package_name__library__",
  "version": "1.0.0",
  "description": "Package description",
  "homepage": "https://skyleague.github.io/node-standards/",
  "repository": {
    "type": "git",
    "url": "https://github.com/skyleague/node-standards.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./.dist/index.js",
    "./package.json": "./package.json",
